{
    int a, b;
    printf("\u8BF7\u8F93\u5165\u4E24\u4E2A\u6574\u6570\uFF1A");
    scanf("%d %d", &a, &b);
    ...
    printf("%d\u548C%d\u7684\u6700\u5927\u516C\u7EA6\u6570\u662F%d\\n", a, b, c);
